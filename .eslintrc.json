{
  "extends": ["airbnb", "airbnb -base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": 0,
    "no-param-reassign": [2, { "props": false }],
    "prefer-destructuring": 0,
    "treatUndefinedAsUnspecified": true,
    "arrow-body-style": 0,
    "comma-dangle": 0
  },
  "env": {
    "commonjs": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "es6": true
  }
}
